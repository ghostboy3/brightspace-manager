{
  "name": "Honk Helper",
  "description": "procrastination pet!!!",
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "scripting",
    "declarativeNetRequestWithHostAccess",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "version": "1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "gooseIcon.jpg"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ],

  "icons": {
    "128": "/assets/goosL.png"
  },
  "options_page": "options.html"
}